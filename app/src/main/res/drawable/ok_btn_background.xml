<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_background" />
            <stroke android:width="1dp" android:color="@color/ok_btn_color_stroke" android:dashWidth="3dp" android:dashGap="3dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/color_background" />
            <stroke android:width="1dp" android:color="@color/ok_btn_color_stroke_pressed" android:dashWidth="3dp" android:dashGap="3dp" />
        </shape>
    </item>
</selector>